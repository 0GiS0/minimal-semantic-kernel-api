{
	"name": "Minimal Semantic Kernel API",
	"dockerComposeFile": "docker-compose.yaml",
	"service": "workspace",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-semantic-kernel.semantic-kernel",
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"ms-dotnettools.csdevkit",
				"humao.rest-client"
			]
		}
	},
	"forwardPorts": [
		5170,
		6333
	]
}